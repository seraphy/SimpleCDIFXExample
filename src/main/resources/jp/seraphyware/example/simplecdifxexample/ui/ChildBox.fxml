<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="root" maxHeight="-Infinity" maxWidth="1.7976931348623157E308"
	minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="jp.seraphyware.example.simplecdifxexample.ui.ChildBoxController">
	<children>
		<TextField fx:id="textField" prefWidth="200.0" VBox.vgrow="ALWAYS" />
		<HBox alignment="CENTER_RIGHT" VBox.vgrow="NEVER">
			<children>
				<Button mnemonicParsing="false" onAction="#onFetch" text="%btn.childbox.fetch" />
				<Button mnemonicParsing="false" onAction="#onShow" text="%btn.childbox.show" />
			</children>
			<VBox.margin>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</VBox.margin>
		</HBox>
	</children>
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
	xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			VBox.vgrow="ALWAYS">
			<children>
				<StackPane alignment="TOP_CENTER" maxWidth="1.7976931348623157E308"
					VBox.vgrow="ALWAYS">
					<children>
						<fx:include fx:id="childBox" source="ChildBox.fxml" />
					</children>
				</StackPane>
			</children>
		</VBox>
		<HBox alignment="CENTER_RIGHT" VBox.vgrow="NEVER">
			<children>
				<Button mnemonicParsing="false" onAction="#onClose" text="Close" />
			</children>
			<padding>
				<Insets bottom="3.0" left="3.0" right="20.0" top="3.0" />
			</padding>
		</HBox>
	</children>
</VBox>
